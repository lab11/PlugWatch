{
    "name": "powerwatch-deployment-manager",
    "version": "0.0.1",
    "description": "Pays powerwatch users",
    "private": true,
    "dependencies": {
        "pg": "^7.3.0",
        "request": "2.88.0",
        "pg-format": "",
        "commander": "",
        "async": "2.6.1",
        "sort-object": "3.0.2",
        "body-parser": "^1.18.3",
        "twilio": "3.29.2"
    },
    "scripts": {
