{
    "name": "powerwatch-influx-poster",
    "version": "0.0.1",
    "description": "Consume SSE events and post them to Influxdb",
    "private": true,
    "dependencies": {
        "particle-api-js": "^7.2.0",
        "influx-poster": "^1.1.0",
        "commander":"",
        "lab11-powerwatch-parser":"0.0.4"
    },
