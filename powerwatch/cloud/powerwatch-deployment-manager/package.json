{
    "name": "powerwatch-deployment-manager",
    "version": "0.0.1",
    "description": "Takes surveyCTO surveys and uses them to initialize the deployment database",
    "private": true,
    "dependencies": {
        "pg": "^7.3.0",
        "request": "2.88.0",
        "pg-format": "",
        "commander": "",
        "jsqr": "1.1.1",
        "jpeg-js": "0.3.4",
        "csvtojson": "2.0.8",
        "async": "2.6.1",
        "deep-diff": "1.0.2",
        "firebase-admin": "7.0.0",
        "isomorphic-git": "0.51.12"
    },
    "scripts": {
        "start": "./powerwatch-deployment-manager.js -d /etc/config/postgres-config.json -u /etc/config/postgres/username -p /etc/config/postgres/password -s /etc/config/survey-config.json -U /etc/config/survey/username -P /etc/config/survey/password -o /etc/config/oink-config.json -a /etc/config/oink/oink_service_account.json"
    },
    "engines": {
        "node": ">=4.3.2"
    }
}
